<?xml version="1.0" encoding="UTF-8"?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <!-- Action Metadata Guess Config -->
        <service id="klipper_api_import.metadata.guess.actions_metadata"
                 class="Klipper\Bundle\ApiImportBundle\Metadata\Guess\GuessActionsMetadata"
                 public="false">
            <tag name="klipper_metadata.guess" />
        </service>

        <service id="klipper_api_import.metadata.guess.action_metadata"
                 class="Klipper\Bundle\ApiImportBundle\Metadata\Guess\GuessActionMetadata"
                 public="false">
            <tag name="klipper_metadata.guess" />
        </service>
    </services>
</container>
